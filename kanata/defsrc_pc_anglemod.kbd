;; angle-mod: the ISO key (a.k.a. LSGT or 102 key) becomes Z

(defsrc
  `     1    2    3    4    5         6    7    8    9    0    -
  tab   q    w    e    r    t         y    u    i    o    p    [
  caps  a    s    d    f    g         h    j    k    l    ;    '
  lsft  <    z    x    c    v    b    n    m    ,    .    /    rsft
                  lalt          spc             ralt
)

